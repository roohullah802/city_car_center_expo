{
  "expo": {
    "name": "City Car Center",
    "slug": "city_car_center_expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "citycarcenterexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png", 
      "backgroundColor": "#FFFFFF"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.roohullah1.city_car_center_expo"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 500,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "dark": {
            "backgroundColor": "#FFFFFF",
            "image": "./assets/splash.png"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "5fb00395-ffbb-45e7-8b2c-f7c9f43c4ddb"
      }
    }
  }
}
